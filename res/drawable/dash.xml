<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    >

    <item
        android:left="8dp"
        android:right="8dp"
        >

		<shape
		    android:shape="line"
		    >

		    <stroke
                android:color="#FFB0B0B0"
                android:width="1dp"
                android:dashGap="5dp"
                android:dashWidth="5dp"
			/>
	
		    <size
		        android:height="1dp"
		    />
		</shape>
    </item>
</layer-list>